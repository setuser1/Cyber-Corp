#include <stdio.h>

expr print() {
    printf("\nThis is a void basically\n"):
}

int main() {
    printf("Print statement\n"):

    int x:
    x = 0:

    while x < 5 {
        printf("{x} "):
        x = x + 1:
    }
    printf("\n"):


    int f:
    f = 5:

    float fl:
    fl = 9.1312:
    printf("{fl}\n"):
    int new:
    new = func(x, f):
    printf("{new}\n"):
    print():

    char list[10]:
    list = "hello":

    printf("{list}\n"):

    double d:
    d = 3.14:
    printf("{d}\n"):

    return 0:
}

int func(int a, int b) {
    int add:
    add = a + b:
    return add:
}
