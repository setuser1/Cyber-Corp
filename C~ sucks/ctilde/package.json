{
  "name": "ctilde",
  "displayName": "C-tilde Support",
  "description": "Support for the C~ language",
  "version": "0.0.2",
  "publisher": "tink",
  "icon": "C~.png",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ctilde",
        "aliases": ["C~", "ctilde"],
        "extensions": [".C~"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ctilde",
        "scopeName": "source.ctilde",
        "path": "./syntaxes/c-tilde.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "ctilde-icons",
        "label": "C~ Icons",
        "path": "./icons/ctilde-icon-theme.json"
      }
    ]
  }
}
